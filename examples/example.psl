def add(x, y) {
	return (x + y);
}

def sub(x, y) {
	return (x - y);
}

def mlt(x, y) {
	return (x * y);
}

def div(x, y) {
	return (x / y);
}

def foo(x) {
	if (x == 1) {
		return True;
	}
	cnt = 0;
	while (cnt < 11) { cnt++; }
	while (cnt > 0) { cnt--; }
	while (cnt <= 10) { cnt++; }
	while (cnt >= 0 ){ cnt--; }
	while (cnt != 10) { cnt++; }
	while (cnt == 10) { cnt--; }
}

